<?xml version="1.0" encoding="UTF-8"?>
<table>
    <column>
        <name>id</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>usuario_id</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <column>
        <name>curso_id</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <column>
        <name>tipo</name>
        <type>character varying(20)</type>
        <null>NO</null>
    </column>
    <column>
        <name>nombre</name>
        <type>character varying(200)</type>
        <null>NO</null>
    </column>
    <column>
        <name>nota</name>
        <type>double precision</type>
        <null>YES</null>
    </column>
    <column>
        <name>nota_maxima</name>
        <type>double precision</type>
        <default>10.00</default>
        <null>YES</null>
    </column>
    <column>
        <name>fecha</name>
        <type>date</type>
        <null>YES</null>
    </column>
    <constraint>
        <name>cl_calificaciones_pkey</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>
    <constraint>
        <name>cl_calificaciones_usuario_fkey</name>
        <type>FOREIGN KEY (usuario_id) REFERENCES cl_usuarios (id)</type>
    </constraint>
    <constraint>
        <name>cl_calificaciones_curso_fkey</name>
        <type>FOREIGN KEY (curso_id) REFERENCES cl_cursos (id)</type>
    </constraint>
</table>